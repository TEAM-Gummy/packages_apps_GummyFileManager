    <color name="ash_multiline_comment_color">#ff7f9fbf</color>
    <color name="ash_keyword_color">#ffd2568a</color>
    <color name="ash_quoted_string_color">#ff4Ab3b6</color>
    <color name="ash_variable_color">#ff91bcf8</color>

